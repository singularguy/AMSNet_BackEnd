<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scy.springbootinit.mapper.FileEntityMapper">
    <resultMap id="BaseResultMap" type="com.scy.springbootinit.model.entity.FileEntity">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="userId" column="user_id" jdbcType="BIGINT"/>
        <result property="text" column="text" jdbcType="VARCHAR"/>
        <result property="color" column="color" jdbcType="VARCHAR"/>
        <result property="timestamp" column="timestamp" jdbcType="BIGINT"/>
        <result property="imageFile" column="image_file" jdbcType="BLOB"/>
    </resultMap>
    <insert id="insertFileEntity"></insert>
</mapper>
